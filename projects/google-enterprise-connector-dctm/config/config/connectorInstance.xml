<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="DctmConnectorInstance"
		class="com.google.enterprise.connector.dctm.DctmConnector">
		<property name="docbase" value="${docbase}" />
		<property name="login" value="${login}" />
		<property name="password" value="${password}" />
		<property name="clientX" value="${clientX}" />
		<property name="queryStringUnboundedDefault" value="${queryStringUnboundedDefault}" />
		<property name="queryStringBoundedDefault" value="${queryStringBoundedDefault}" />
		<property name="queryStringAuthoriseDefault" value="${queryStringAuthoriseDefault}" />
		<property name="attributeName" value="${attributeName}" />
		<property name="webtopServerUrl" value="${webtopServerUrl}" />
	</bean>
</beans>
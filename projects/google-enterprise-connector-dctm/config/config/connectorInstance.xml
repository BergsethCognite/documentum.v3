<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="DctmConnectorInstance"
		class="com.google.enterprise.connector.dctm.DctmConnector">
		<property name="docbase" value="${docbase}" />
		<property name="login" value="${login}" />
		<property name="password" value="${password}" />
		<property name="clientX" value="com.google.enterprise.connector.dctm.dctmdfcwrap.DmClientX" />
		<property name="authenticationType" value="${authenticationType}" />
		<property name="webtopServerUrl" value="${webtopServerUrl}" />
		<property name="additionalWhereClause" value="${additionalWhereClause}" />
	</bean>
</beans>